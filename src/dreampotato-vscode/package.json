{
  "name": "dreampotato",
  "displayName": "DreamPotato",
  "description": "Developer tools for LC86K assembly (used by the Dreamcast VMU)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [{
      "id": "lc86k",
      "aliases": ["lc86k Assembly", "lc86k"],
      "extensions": [".s",".asm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lc86k",
      "scopeName": "source.lc86k",
      "path": "./syntaxes/lc86k.tmLanguage.json"
    }]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "typescript": "^5.0.0"
  }
}
